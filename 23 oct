class Solution{
public void printpattern(int n){
for(int i=n;i<=1;i--){
for(int j=1;j<=i;j++){
System.out.print("* ");}
System.out.println();
}
}
}


class Solution{
public void printpattern(int n){
for(int i=n;i<=1;i--){
for(int j=1;j<=i;j++){
System.out.print("* ");{
System.out.println();}
}
}


import java.util.*;
public class Solution{
public static void main(String[] args){
Scanner sc = new Scanner(System.in);
int n = sc.nextInt();
String row = "* ".repeat(n).trim();
for(int i=0;i<=n;i++){
System.out.println(row);}
}
}

