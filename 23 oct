result*=i;
}
return result;
}
}

class Solution{
public int computeGcd(int a,int b){
while(b!=0){
int temp =b;
b = a%b;
a = temp;
}
return a;
}
}

class Solution{
public int computeGCD(int a,int b){
while(b!=0){
int temp =b;
b = a%b;
a = temp;}
return a;
}
}

class Solution{
public static claculateLCM(int a,int b){
int gcd =a;
int temp =b;
while(temp !=0){
int t = temp;
temp = gcd%temp;
gcd =t;}
return (a+b)/gcd;}
}


class Solution{
public static calculateLCM(int a,int b){
int gcd =a;
int temp =b;
while(temp !=0){
int t = temp;
temp = gcd%temp;
gcd =t;}
return (a+b)/temp;
}
}

class Solution{
public class calculateLCM(int a,int b){
int gcd =a;
int temp =b;
while(temp !=0){
int t =temp;
temp =gcd%temp;
gcd =t;
}
return (a+b)/temp;
}
}


class Solution{
void printDivisors(int n){
for(int i=1;i<=n;i++){
if(n%i==0){
System.out.println(i+" ");
}
}
}
}

